  </div>


  <footer class="footer">
    {{#each footer}}
    <div class="footer__block">
      <strong class="footer__title">{{title}}</strong>
      {{#each links}}
      <a href="{{url}}" class="footer__content">{{text}}</a>
      {{/each}}
    </div>
    {{/each}}
  </footer>
</div>